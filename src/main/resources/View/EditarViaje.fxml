<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.EditarViajeF" prefHeight="400.0" prefWidth="400.0">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Editar Viaje" style="-fx-font-size: 20px;" />
        <Label layoutX="20.0" layoutY="60.0" text="Destino:" />
        <TextField fx:id="txtDestino" layoutX="100.0" layoutY="60.0" prefWidth="200.0" />
        <Label layoutX="20.0" layoutY="100.0" text="Fecha Salida:" />
        <DatePicker fx:id="dpFechaSalida" layoutX="100.0" layoutY="100.0" />
        <Label layoutX="20.0" layoutY="140.0" text="Fecha Regreso:" />
        <DatePicker fx:id="dpFechaRegreso" layoutX="100.0" layoutY="140.0" />
        <Label layoutX="20.0" layoutY="180.0" text="Precio:" />
        <TextField fx:id="txtPrecio" layoutX="100.0" layoutY="180.0" prefWidth="200.0" />
        <Label layoutX="20.0" layoutY="220.0" text="Plazas:" />
        <TextField fx:id="txtPlazas" layoutX="100.0" layoutY="220.0" prefWidth="200.0" />
        <Button layoutX="100.0" layoutY="280.0" text="Guardar" onAction="#handleGuardar" />
        <Button layoutX="200.0" layoutY="280.0" text="Cancelar" onAction="#handleCancelar" />
    </children>
</AnchorPane>
